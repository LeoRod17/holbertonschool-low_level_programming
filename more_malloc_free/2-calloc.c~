#include "main.h"

/**
 **_calloc - a function to alocate the memory of an array with malloc
 *@nemb: - the number of elements on the array
 *@size: -the weight on byte of the variable
 *Return: 0
 */
void *_calloc(unsigned int nmemb, unsigned int size)
{
char *p;
if (nmemb < 1 || size < 1)
{
return (NULL);
}
p = malloc(size * nmemb);
return(p);
}
