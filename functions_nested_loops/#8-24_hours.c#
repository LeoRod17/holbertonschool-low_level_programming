#include "main.h"
/**
 *jack_bauer - this function prints every minute and hour of the day
 *
 *Return: 0
 */

void jack_bauer(void)
{
int h1;
int h2 = 48;
int m1;
int m2;
for (h1 = 48; h1 < 51; h1++)
{
while (h2 < 58)
{

for (m1 = 48; m1 < 54; m1++)
{
for (m2 = 48; m2 < 58; m2++)
{
putchar(h1);
putchar(h2);
putchar(':');
putchar(m1);
putchar(m2);
putchar('\n');
}
}
}
}
}
